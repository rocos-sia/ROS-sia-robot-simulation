<?xml version="1.0" ?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" 
 name="siasun_GCR10">

<material name ="Blue">
   <color rgba="0.137 0.49412 0.74902 1"/>
 </material>
<material name ="Black">
   <color rgba="0 0 0 0.8"/>
 </material>
<material name ="Dark Blue">
   <color rgba="0.098 0.098 0.439 1.0"/>
 </material>
<material name ="light Blue">
   <color rgba="0.387 0.584 0.93 1.0"/>
 </material>
<material name ="White">
   <color rgba="1 1 1 1"/>
 </material>
<material name ="Antique White">
   <color rgba="0.98 0.92 0.84 1"/>
 </material>
<material name ="Dark Gray">
   <color rgba="0.537 0.594 0.74902 1"/>
 </material>
<material name ="Gray">
   <color rgba="0.537 0.594 0.74 1"/>
 </material>
 <material name ="Silver">
    <color rgba="0.588 0.588 0.588 1"/>
 </material>
 <material name ="Red">
    <color rgba="1.00 0 0 1.0"/>
 </material>

<!-- base_link -->
  <link name="base_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://sia_robot/meshes/GCR10/base.stl"/>
      </geometry>
      <material name ="Gray"/>
    </visual>
   <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://sia_robot/meshes/GCR10/base.stl"/>
      </geometry>
    </collision>
  </link>

<!-- link_1 -->
<link name="link_1">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://sia_robot/meshes/GCR10/link1.dae"/>
      </geometry>
      <material name ="Blue"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://sia_robot/meshes/GCR10/link1.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="12.369"/>
      <inertia ixx="0.069552066" ixy="0.000045456" ixz="0.000067368" iyy="0.0682463" iyz="0.000262322" izz="0.054371276"/>
    </inertial>
  </link>
<!-- joint_1 -->
<joint name="joint_1" type="revolute">
    <origin rpy="0 0 ${-pi/2}" xyz="0 0 0.165"/>
    <parent link="base_link"/>
    <child link="link_1"/>
    <axis xyz="0 0 1"/>
    <limit effort="140" lower="${-pi}" upper="${pi}" velocity="${65*pi/180}"/>
  </joint>
<!-- link_2 -->
